		{
			"name": "Daily Summary",
			"item": [
				{
					"name": "Get Daily Nutrition Plan",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Visualization Script",
									"// Visualization Script",
									"const template = `",
									"<h3>Daily Nutrition Summary for {{day}}</h3>",
									"<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\" style=\"border-collapse:collapse;\">",
									"  <thead>",
									"    <tr>",
									"      <th>Meal</th>",
									"      <th>Status</th>",
									"      <th>Meal ID</th>",
									"      <th>Custom Name</th>",
									"      <th>Calories</th>",
									"      <th>Protein</th>",
									"      <th>Carbs</th>",
									"      <th>Fat</th>",
									"    </tr>",
									"  </thead>",
									"  <tbody>",
									"    {{#each meals}}",
									"      <tr>",
									"        <td>{{@key}}</td>",
									"        <td>{{this.status}}</td>",
									"        <td>{{this.meal_id}}</td>",
									"        <td>{{#if this.custom_name}}{{this.custom_name}}{{else}}-{{/if}}</td>",
									"        <td>{{#if this.custom_calories}}{{this.custom_calories}}{{else}}-{{/if}}</td>",
									"        <td>{{#if this.custom_protein}}{{this.custom_protein}}{{else}}-{{/if}}</td>",
									"        <td>{{#if this.custom_carbs}}{{this.custom_carbs}}{{else}}-{{/if}}</td>",
									"        <td>{{#if this.custom_fat}}{{this.custom_fat}}{{else}}-{{/if}}</td>",
									"      </tr>",
									"    {{/each}}",
									"  </tbody>",
									"</table>",
									"<br/>",
									"<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\" style=\"border-collapse:collapse;\">",
									"  <thead>",
									"    <tr>",
									"      <th>Target Calories</th>",
									"      <th>Target Protein</th>",
									"      <th>Target Carbs</th>",
									"      <th>Target Fat</th>",
									"    </tr>",
									"  </thead>",
									"  <tbody>",
									"    <tr>",
									"      <td>{{target_calories}}</td>",
									"      <td>{{target_protein}}</td>",
									"      <td>{{target_carbs}}</td>",
									"      <td>{{target_fat}}</td>",
									"    </tr>",
									"  </tbody>",
									"</table>",
									"`;",
									"",
									"function createPayload() {",
									"    var data = pm.response.json();",
									"    // Convert meals object to array with meal name as key",
									"    var meals = [];",
									"    for (var key in data.meals) {",
									"        if (data.meals.hasOwnProperty(key)) {",
									"            meals.push({",
									"                key: key,",
									"                ...data.meals[key]",
									"            });",
									"        }",
									"    }",
									"    // But for Handlebars, keep as object for @key",
									"    return {",
									"        day: data.day,",
									"        meals: data.meals,",
									"        target_calories: data.target_calories,",
									"        target_protein: data.target_protein,",
									"        target_carbs: data.target_carbs,",
									"        target_fat: data.target_fat",
									"    };",
									"}",
									"pm.visualizer.set(template, createPayload());"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
