								}
							]
						},
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{foodiniUrl}}/v1/daily_summary/meals/2025-10-09?user_id={{userId}}",
							"host": [
								"{{foodiniUrl}}"
							],
							"path": [
								"v1",
								"daily_summary",
								"meals",
								"2025-10-09"
							],
							"query": [
								{
